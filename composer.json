{
    "name": "age-partnership/form-js-validation-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Form Js Validation Bundle",
    "keywords": ["Symfony", "form", "javascript", "validation"],
    "homepage": "https://github.com/AgePartnership/FormJsValidationBundle",
    "authors": [
        {
            "name": "Nicolas Potier",
            "email": "nicolas.potier@acseo.fr"
        },
        {
            "name": "Harry Brook-Dobson",
            "email": "harry.brook-dobson@agepartnership.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/yaml": "^4.4",
        "symfony/dependency-injection": "^4.4"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": { "AgePartnership\\Bundle\\FormJsValidationBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "AgePartnership\\Bundle\\FormJsValidationBundle\\Tests\\": "tests/" }
    },
    "target-dir": "AgePartnership/Bundle/FormJsValidationBundle",
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/validator": "^4.4",
        "symfony/form": "^4.4",
        "symfony/translation": "^4.4"
    }
}
